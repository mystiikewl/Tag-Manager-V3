# Shopify API Configuration
# Copy this file to .env and fill in your actual values

# Your Shopify store domain (e.g., mystore.myshopify.com)
SHOPIFY_STORE_DOMAIN=your-shop-name.myshopify.com

# Your Shopify Admin API access token
# Generate this in your Shopify admin under Settings > Apps and Sales Channels > Develop apps
SHOPIFY_ADMIN_API_TOKEN=shpat_your_admin_api_token_here

# Shopify API version to use
SHOPIFY_API_VERSION=2024-10